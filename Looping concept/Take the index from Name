using System;

namespace ConsoleApp28
{
    class Program
    {
        static void Main(string[] args)
        {
            string value = "Pradheepan";
            for (int i=0; i<value.Length; i++)
            {
                char index = value[i];
                Console.WriteLine(index);
            }
            Console.ReadLine();
        }
    }
}
