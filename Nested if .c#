using System;

namespace ConsoleApp20
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Enter your Mark!");
            int num = Convert.ToInt32(Console.ReadLine());
            if (num < 0 || num > 100)
            {
                Console.WriteLine("it is the Wrong Number.");
            }
            else if (num <= 100 && num >= 80)
            {
                Console.WriteLine("pass in Distincsan");
            }
            else if (num <= 79 && num >= 50)
            {
                Console.WriteLine("pass in c grade");
            }
            else if (num <= 49 && num >= 30)
            {
                Console.WriteLine("pass in s Grade");
            }
            else
            {
                Console.WriteLine("Fail");
            }
            Console.ReadLine();
        }
    }
}
